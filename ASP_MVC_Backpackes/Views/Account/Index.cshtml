@model ASP_MVC_Backpackes.Models.GroupedUserViewModel
<hr style="border-top: 3px double #FF3C9C ">
<div class="container">
    <div class="row">
        <div class="col-sm-4">
            <h4>Admini</h4>
            @foreach (var usera in Model.Admins)
            {
                <p>
                    <strong>
                        @usera.Email<br/>
                    </strong>
                </p>
            }
        </div>
        <div class="col-sm-4">
            <h4>Goście</h4>
            @foreach (var user in Model.Users)
            {
                <p>
                    <strong>
                        @user.Email<br/>
                    </strong>
                </p>
            }
        </div>
        <div class="col-sm-4">
            <h4>Pozostali</h4>
            @foreach (var usero in Model.Other)
            {
                <p>
                    <strong>
                        @usero.Email<br/>
                    </strong>
                </p>
            }
        </div>
    </div>
</div>

